<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PrincipaleController">
   <children>
      <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0">
         <children>
            <TableView fx:id="tabella" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="colNome" minWidth="150.0" prefWidth="75.0" text="Nome" />
                <TableColumn fx:id="colCognome" minWidth="150.0" prefWidth="75.0" text="Cognome" />
                  <TableColumn fx:id="colNumero" minWidth="300.0" prefWidth="75.0" text="Numero" />
              </columns>
            </TableView>
            <HBox alignment="BOTTOM_CENTER" prefWidth="600.0">
               <children>
                  <Button fx:id="btNuovo" mnemonicParsing="false" onAction="#nuovoContatto" prefWidth="80.0" text="Nuovo">
                     <HBox.margin>
                        <Insets right="94.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btModifica" mnemonicParsing="false" onAction="#modificaContatto" prefWidth="80.0" text="Modifica">
                     <HBox.margin>
                        <Insets right="94.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btElimina" mnemonicParsing="false" onAction="#eliminaContatto" prefWidth="80.0" text="Elimina">
                     <HBox.margin>
                        <Insets right="94.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btLogOut" mnemonicParsing="false" onAction="#logOut" prefWidth="80.0" text="Logout" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
